{
  "name": "vue-crud-toolkit",
  "version": "1.0.0",
  "private": false,
  "description": "Vue crud toolkit",
  "author": "Hieunv495",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn build-lib && yarn build-storybook",
    "lint": "vue-cli-service lint",
    "build-lib": "vue-cli-service build --target lib -name vue-crud-toolkit src/main.js",
    "build-storybook": "build-storybook -o docs -s ./src/assets",
    "prepublishOnly": "$npm_execpath build",
    "storybook": "start-storybook -p 6006",
    "vue-devtools": "vue-devtools"
  },
  "main": "dist/vue-crud-toolkit.common.js",
  "unpkg": "dist/vue-crud-toolkit.umd.min.js",
  "cdn": "dist/vue-crud-toolkit.umd.min.js",
  "browser": "dist/vue-crud-toolkit.umd.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "deep-equal": "^2.0.5",
    "core-js": "^3.6.5"
  },
  "peer-dependencies": {
    "vue": "^2.6.11",
    "vuetify": "^2.2.32"
  },
  "devDependencies": {
    "vue": "^2.6.11",
    "vuetify": "^2.2.32",
    "@babel/core": "^7.12.10",
    "@mdi/font": "^5.8.55",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/vue": "^6.1.11",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/devtools": "^5.3.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "faker": "^5.1.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "slugify": "^1.4.6",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.1",
    "vue-cli-plugin-vuetify": "^2.0.8",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/hieunv495/vue-crud-toolkit/issues"
  },
  "homepage": "https://hieunv495.github.io/vue-crud-toolkit",
  "jsdelivr": "dist/vue-crud-toolkit.umd.min.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hieunv495/vue-crud-toolkit.git"
  }
}
